    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Ensure Ig_Date is a datetime column\n",
    "fire_gdf['Ig_Date'] = pd.to_datetime(fire_gdf['Ig_Date'], errors='coerce')\n",
    "\n",
    "# Extract year\n",
    "fire_gdf['Year'] = fire_gdf['Ig_Date'].dt.year\n",
    "\n",
    "# Drop rows with missing years\n",
    "fire_years = fire_gdf.dropna(subset=['Year'])\n",
    "\n",
    "# Make histogram\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.hist(fire_years['Year'], bins=20)  # bins can be adjusted\n",
    "plt.xlabel('Ignition Year')\n",
    "plt.ylabel('Number of Fire Events')\n",
    "plt.title('Distribution of Fire Events by Ignition Year')\n",
    "plt.show()"
